{
  "name": "discojs",
  "version": "2.1.0",
  "description": "Easiest way to use the Discogs API in Javascript/Typescript",
  "author": "aknorw",
  "license": "MIT",
  "keywords": [
    "discogs",
    "api",
    "client"
  ],
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "files": [
    "lib"
  ],
  "types": "./lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "pretty": "prettier --config .prettierrc --write '{models,src}/**/*.{ts,json}'",
    "lint": "eslint --ext .ts,.json models/ src/",
    "test": "cross-env NODE_ENV=test jest",
    "docs": "typedoc --inputFiles src/index.ts --exclude \"**/*+(.spec).ts\" --excludeNotExported --excludePrivate --stripInternal",
    "prebuild": "yarn run clean",
    "build": "cross-env NODE_ENV=production rollup -c",
    "build-dev": "cross-env NODE_ENV=development rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aknorw/discojs.git"
  },
  "homepage": "https://github.com/aknorw/discojs#readme",
  "bugs": {
    "url": "https://github.com/aknorw/discojs/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.{ts,json}": [
      "yarn pretty",
      "yarn lint"
    ]
  },
  "dependencies": {
    "bottleneck": "^2.19.5",
    "cross-fetch": "^3.0.6",
    "oauth-1.0a": "^2.2.6",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "fp-ts": "^2.8.5",
    "husky": "^4.3.0",
    "io-ts": "^2.2.12",
    "jest": "^26.6.1",
    "lint-staged": "^10.5.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.1",
    "rollup-plugin-ts": "^1.3.7",
    "ts-jest": "^26.4.3",
    "tslib": "^2.0.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.0.5"
  }
}
